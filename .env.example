# Supabase Database Connection (Required)
SUPABASE_DATABASE_URL=postgresql://postgres:[PASSWORD]@[PROJECT_ID].supabase.co:5432/postgres

# Supabase Project Settings (Required)
SUPABASE_URL=https://[PROJECT_ID].supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Supabase Anon Key (Required for frontend)
SUPABASE_ANON_KEY=your_anon_key_here

# Storage Bucket Name (default: products)
SUPABASE_STORAGE_BUCKET=products

# Node Version (recommended for Vercel)
# Set in Vercel dashboard or package.json engines
# NODE_VERSION=20
